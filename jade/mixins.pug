mixin breadcrumbs(list)
    ul.breadcrumbs
        for item, i in list
            li
                - if (i === (list.length - 1)) {
                    span= item
                - } else {
                    a.crumb_link(href= (item === 'Главная' ? 'index.html' : ''))= item
